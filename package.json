{
    "name": "meepo-jssdk",
    "version": "1.1.1",
    "description": "wechat jssdk for angular",
    "main": "./bundles/meepo.umd.js",
    "module": "./esm5/meepo.js",
    "es2015": "./esm2015/meepo.js",
    "typings": "./meepo.d.ts",
    "scripts": {
        "build": "bash ./build.sh",
        "git": "git add . && git commit -m 'save to back' && git push origin master",
        "test": "node index.js",
        "demo": "ng serve --port 4010 --open",
        "server": "pm2 start index.js --name meepo",
        "reload": "pm2 restart all && pm2 logs meepo",
        "delete": "pm2 delete all",
        "logs": "pm2 logs meepo"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/meepobrother/meepo-jssdk.git"
    },
    "keywords": [],
    "author": "imeepos",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/meepobrother/meepo-jssdk/issues"
    },
    "homepage": "https://github.com/meepobrother/meepo-jssdk/README.md",
    "devDependencies": {
        "@angular/cli": "^1.6.1",
        "@angular/compiler": "5.2.0-beta.1",
        "@angular/compiler-cli": "5.2.0-beta.1",
        "@angular/platform-browser": "5.2.0-beta.1",
        "@angular/platform-browser-dynamic": "5.2.0-beta.1",
        "@types/node": "^8.5.1",
        "less": "^2.7.3",
        "node-sass": "^4.7.2",
        "nodemon": "^1.13.3",
        "pm2": "^2.9.1",
        "rollup": "^0.52.1",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-replace": "^2.0.0",
        "rollup-plugin-sourcemaps": "^0.4.2",
        "rollup-plugin-uglify": "^2.0.1",
        "ts-node": "^4.0.2",
        "typescript": "~2.4.2"
    },
    "dependencies": {
        "@angular/common": "5.2.0-beta.1",
        "@angular/core": "5.2.0-beta.1",
        "@angular/forms": "5.2.0-beta.1",
        "meepo-axios": ">=1.2.1",
        "meepo-core": ">=3.0.4",
        "reflect-metadata": "^0.1.10",
        "rxjs": "^5.5.5",
        "zone.js": "0.8.4"
    }
